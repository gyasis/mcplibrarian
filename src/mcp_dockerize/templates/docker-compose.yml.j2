# Docker Compose config for {{ service_name }} â€” generated by mcplibrarian
# Deployment pattern: {{ deployment_pattern }}
# Do not edit manually; regenerate with: mcplibrarian wrap <path>

services:
  {{ service_name }}:
    build:
      context: {{ build_context }}
      dockerfile: {{ dockerfile_path }}
    stdin_open: true
    tty: false
    env_file:
      - .env
{% if volumes %}
    volumes:
{% for vol in volumes %}
      - type: bind
        source: {{ vol.source }}
        target: {{ vol.target }}
{% if vol.read_only %}
        read_only: true
{% endif %}
{% endfor %}
{% endif %}
